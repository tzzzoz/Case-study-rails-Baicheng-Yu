<%= form_for(@pro_sante) do |f| %>
  <% if @pro_sante.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@pro_sante.errors.count, "error") %> prohibited this pro_sante from being saved:</h2>

      <ul>
      <% @pro_sante.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :nom %><br>
    <%= f.text_field :nom %>
  </div>
  <div class="field">
    <%= f.label :prenom %><br>
    <%= f.text_field :prenom %>
  </div>

  <%= f.fields_for :adresse do |a| %>
      <h3>Adresse:</h3>
      <div class="field">
        <%= a.label "Adresse Name".to_sym %><br />
        <%= a.text_field :name %>
      </div>
      <div class="field">
        <%= a.label :latitude %><br />
        <%= a.text_field :latitude %>
      </div>
      <div class="field">
        <%= a.label :longitude %><br />
        <%= a.text_field :longitude %>
      </div>
  <% end %>

  <div class="field">
    <%= f.label :specialte %><br>
    <%= f.text_field :specialte %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
